{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"],
      "tests": ["tests/*"],
      "@env": [".env"],
      "@components/*": ["src/components/*"],
      "@common/*": ["src/common/*"],
      "@assets/*": ["./assets/*"],
      "@hooks/*": ["src/hooks/*"]
    }
  }
}
